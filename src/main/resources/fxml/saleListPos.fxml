<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox prefHeight="32.0" prefWidth="1000.0" styleClass="itemBorder" stylesheets="@../styles/Style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mohware.mills.pos.saleListController">
   <children>
      <Label fx:id="receiptNo" prefHeight="16.0" prefWidth="120.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Receipt No">
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
      </Label>
      <Label fx:id="dateSale" layoutX="20.0" layoutY="20.0" prefHeight="16.0" prefWidth="120.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Date">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="timeSale" layoutX="105.0" layoutY="10.0" prefHeight="16.0" prefWidth="120.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Time">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="mpesa" alignment="CENTER" layoutX="190.0" layoutY="10.0" prefHeight="16.0" prefWidth="140.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="MPESA">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="cash" alignment="CENTER" layoutX="285.0" layoutY="20.0" prefHeight="16.0" prefWidth="140.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Cash" textAlignment="CENTER">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="total" alignment="CENTER" prefHeight="16.0" prefWidth="150.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Total">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="code" alignment="CENTER" layoutX="721.0" layoutY="20.0" prefHeight="16.0" prefWidth="120.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="MPESA Code">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
   </children>
</HBox>
