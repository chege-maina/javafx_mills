<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="32.0" prefWidth="800.0" styleClass="itemBorder" stylesheets="@../styles/Style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mohware.mills.dashboard.productsList">
   <children>
      <Label fx:id="prodCode" prefHeight="17.0" prefWidth="200.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Product Code">
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
      </Label>
      <Label fx:id="prodName" layoutX="20.0" layoutY="20.0" prefHeight="17.0" prefWidth="200.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Product Name">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="prodCat" layoutX="105.0" layoutY="10.0" prefHeight="17.0" prefWidth="150.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Category">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="prodBp" layoutX="190.0" layoutY="10.0" prefHeight="17.0" prefWidth="150.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Buying Price">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="prodSp" layoutX="285.0" layoutY="20.0" prefHeight="17.0" prefWidth="150.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Selling Price">
         <font>
            <Font name="Cambria Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </Label>
      <Button fx:id="action" mnemonicParsing="false" onMousePressed="#click" prefHeight="25.0" prefWidth="80.0" styleClass="search-button" stylesheets="@../styles/Style.css" text="Edit" textFill="#c0d6df">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <HBox.margin>
            <Insets right="5.0" top="3.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
