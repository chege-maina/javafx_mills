<HBox alignment="CENTER_LEFT" prefHeight="21.0" prefWidth="442.0" styleClass="itemBorder" stylesheets="@../styles/Style.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mohware.mills.pos.ReceiptItemsController">
   <children>
      <Label fx:id="itmName" prefHeight="17.0" prefWidth="158.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="Kim Jong IL Sister's Diss America">
