            <Font name="Cambria" size="12.0" />
         </font>
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Label>
      <Label fx:id="itmPrice" minHeight="17.0" prefWidth="70.0" styleClass="label-text1" stylesheets="@../styles/Style.css" text="1,000,006.57">
